    .then(res => {\r\n        resolve(res.data)\r\n      })\r\n      .catch(err => {\r\n        reject(err.data)\r\n      })\r\n  })\r\n}\r\nexport function getFile (url, params) {\r\n  return new Promise((resolve, reject) => {\r\n    \r\n    axios({\r\n      url, \r\n      params,\r\n      method: 'get',\r\n      responseType:'blob',\r\n      headers:{\r\n        ContentType:'multipart/form-data'\r\n      }\r\n    }).then(res => {\r\n      \r\n      resolve(res.data)\r\n    }).catch(err => {\r\n      reject(err.data)\r\n    })\r\n  })\r\n}\r\n      "]}]}