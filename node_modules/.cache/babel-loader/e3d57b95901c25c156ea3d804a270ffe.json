AAA,IAAA,EAAA,aADA;AAEA,UAAA,IAAA,EAAA,aAFA;AAGA,UAAA,KAAA,EAAA,QAHA;AAIA,UAAA,UAAA,EAAA,MAJA;AAKA,UAAA,WAAA,EAAA;AALA,SAfA,EAsBA;AACA,UAAA,IAAA,EAAA,WADA;AAEA,UAAA,IAAA,EAAA,WAFA;AAGA,UAAA,KAAA,EAAA,QAHA;AAIA,UAAA,UAAA,EAAA,MAJA;AAKA,UAAA,WAAA,EAAA;AALA,SAtBA;AALA,OATA,EA6CA;AACA,QAAA,IAAA,EAAA,YADA;AAEA,QAAA,KAAA,EAAA,MAFA;AAGA,QAAA,IAAA,EAAA,SAHA;AAIA,QAAA,IAAA,EAAA;AAJA,OA7CA,EAmDA;AACA,QAAA,KAAA,EAAA,MADA;AAEA,QAAA,IAAA,EAAA,kBAFA;AAGA,QAAA,IAAA,EAAA,UAHA;AAIA,QAAA,IAAA,EAAA;AAJA,OAnDA,EAyDA;AACA,QAAA,KAAA,EAAA,MADA;AAEA,QAAA,IAAA,EAAA,MAFA;AAGA,QAAA,IAAA,EAAA,uBAHA;AAIA,QAAA,IAAA,EAAA;AAJA,OAzDA,CAHA;AAmEA,MAAA,KAAA,EAAA;AAnEA,KAAA;AAqEA,GAjFA;AAkFA,EAAA,OAAA,EAAA;AACA,IAAA,GADA,iBACA;AACA,WAAA,UAAA,GAAA,CAAA,KAAA,UAAA;AACA,KAHA;AAIA;AACA,IAAA,SALA,qBAKA,IALA,EAKA;AACA;AACA,WAAA,MAAA,CAAA,MAAA,CAAA,YAAA,EAAA,IAAA;AACA;AARA;AAlFA,CAAA","sourcesContent":["<template>\r\n<div class=\"el\">\r\n  <!-- <i class=\"el-icon-s-unfold\" @click=\"isC()\"></i> -->\r\n <el-menu\r\n    router\r\n    default-active=\"1\"\r\n    class=\"el-menu-vertical-demo\"\r\n    background-color=\"#5683df\"\r\n    text-color=\"white\"\r\n    active-text-color=\"gold\"\r\n    :collapse=\"isCollapse\"\r\n  >\r\n    <el-menu-item\r\n      :index=\"item.path\"\r\n      v-for=\"item in noChildren\"\r\n      :key=\"item.path\"\r\n      @click=\"clickMenu(item)\"\r\n    >\r\n      <i :class=\"'el-icon-' + item.icon\" class=\"icon\"></i>\r\n      <span slot=\"title\">{{ item.label }}</span>\r\n    </el-menu-item>\r\n    <el-submenu\r\n      :index=\"item.label\"\r\n      v-for=\"(item, index) in hasChildren\"\r\n      :key=\"index\"\r\n    >\r\n      <template slot=\"title\">\r\n        <i :class=\"'el-icon-' + item.icon\" class=\"icon\"></i>\r\n        <span>{{ item.label }}</span>\r\n      </template>\r\n      <el-menu-item-group>\r\n        <el-menu-item\r\n          :index=\"subItem.path\"\r\n          v-for=\"(subItem, subIndex) in item.children\"\r\n          :key=\"subIndex\"\r\n          @click=\"clickMenu(subItem)\"\r\n        >\r\n          {{ subItem.label }}\r\n        </el-menu-item>\r\n      </el-menu-item-group>\r\n    </el-submenu>\r\n  </el-menu>\r\n</div>\r\n \r\n</template>\r\n\r\n<script>\r\nimport VerticalToggle from '../../store/VerticalToggle' \r\nimport Vue from 'vue'\r\nVue.component(VerticalToggle.name, VerticalToggle)\r\nexport default {\r\n  computed: {\r\n    // 获取菜单对象中不包含子级的菜单\r\n    noChildren() {\r\n      return this.asideMenu.filter((item) => !item.children);\r\n    },\r\n    // 获取菜单对象中包含子级的菜单\r\n    hasChildren() {\r\n      return this.asideMenu.filter((item) => item.children);\r\n    },\r\n  },\r\n  data() {\r\n    return {\r\n      isCollapse:false,\r\n      // 配置菜单路由\r\n      asideMenu: [\r\n        // 设置子级菜单路由\r\n        {\r\n          path: '/home/index',\r\n          name: 'index',\r\n          label: '可视化分析',\r\n          icon: 's-home'\r\n          \r\n        },\r\n        {\r\n          path: \"/home\",\r\n          label: \"信息管理\",\r\n          icon: \"message\",\r\n          name:\"home\",\r\n          children: [\r\n            {\r\n              path: \"userInfo\",\r\n              name: \"userInfo\",\r\n              label: \"用户信息管理\",\r\n              parentName: 'home',\r\n              parentLabel: '信息管理',\r\n            },\r\n            {\r\n              path: \"jobInfo\",\r\n              name: \"jobInfo\",\r\n              label: \"职位信息管理\",\r\n              parentName: 'home',\r\n              parentLabel: '信息管理',\r\n            },\r\n            {\r\n              path: \"companyInfo\",\r\n              name: \"companyInfo\",\r\n              label: \"企业信息管理\",\r\n              parentName: 'home',\r\n              parentLabel: '信息管理',\r\n            },\r\n            {\r\n              path: \"skillInfo\",\r\n              name: \"skillInfo\",\r\n              label: \"技能信息管理\",\r\n              parentName: 'home',\r\n              parentLabel: '信息管理',\r\n            },\r\n          ],\r\n        },\r\n        {\r\n          path: \"/home/role\",\r\n          label: \"角色管理\",\r\n          icon: \"setting\",\r\n          name:\"role\",\r\n        },\r\n        {\r\n          label: \"简历管理\",\r\n          path: \"/home/jianLiInfo\",\r\n          icon: \"document\",\r\n          name:\"jianLiInfo\",\r\n        },\r\n        {\r\n          label: \"审核管理\",\r\n          icon: \"help\",\r\n          path: \"/home/applicationInfo\",\r\n          name:\"applicationInfo\",\r\n        },\r\n      ],\r\n      state: \"\",\r\n    };\r\n  },\r\n  methods: {\r\n      isC() {\r\n      this.isCollapse = !this.isCollapse;\r\n    },\r\n    // clickMenu方法获取用户点击左侧导航栏的item状态\r\n    clickMenu(item) {\r\n      // 调用store中的tab.js文件定义的selectMenu方法\r\n      this.$store.commit(\"selectMenu\", item);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.el {\r\n margin-top: -2px;\r\n}\r\n.el-icon-s-unfold{\r\n  color: yellow;\r\n  margin-left: 20px;\r\n}\r\n.el-menu-vertical-demo:not(.el-menu--collapse) {\r\n    width: auto;\r\n    min-height: 400px;\r\n  }\r\n.el-menu{\r\n    border: none;\r\n  }\r\n.icon{\r\n    color: gold;\r\n    font-size: 20px;\r\n  }\r\n</style>\r\n\r\n"],"sourceRoot":"src/components/Breadcrumb"}]}